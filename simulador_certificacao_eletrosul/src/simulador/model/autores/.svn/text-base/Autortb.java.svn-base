package simulador.model.autores;

public class Autortb {
	private int id_AutorTB;
	private String nome_AutorTB;
	private int subestacao_AutorTB;
	public Autortb(int idAutorTB, String nomeAutorTB, int subestacaoAutorTB) {
		super();
		id_AutorTB = idAutorTB;
		nome_AutorTB = nomeAutorTB;
		subestacao_AutorTB = subestacaoAutorTB;
	}
	public int getId_AutorTB() {
		return id_AutorTB;
	}
	public void setId_AutorTB(int idAutorTB) {
		id_AutorTB = idAutorTB;
	}
	public String getNome_AutorTB() {
		return nome_AutorTB;
	}
	public void setNome_AutorTB(String nomeAutorTB) {
		nome_AutorTB = nomeAutorTB;
	}
	public int getSubestacao_AutorTB() {
		return subestacao_AutorTB;
	}
	public void setSubestacao_AutorTB(int subestacaoAutorTB) {
		subestacao_AutorTB = subestacaoAutorTB;
	}
	
}
